
<%- include('header')%>
<!-- MAIN CONTENT-->
<div class="main-content">
    <div class="section__content section__content--p30">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="table-responsive table--no-card m-b-30">
                        <table class="table table-borderless table-striped table-earning">
                            <thead>
                                <tr>
                                    
                                    <th><img src="<%= locals.admin.image%>" alt="image" width="400" height="400 "></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    
                                <tr>
                                    <th>Name</th>
                                    <th><%= locals.admin.name%></th>
                                </tr>
                                <tr>
                                    <th>Email</th>
                                    <th><%= locals.admin.email%></th>
                                </tr>
                                <tr>
                                    <th>Gender</th>
                                    <th><%= locals.admin.gender%></th>
                                </tr>
                                <tr>
                                    <th>Hobby</th>
                                    <th><%= locals.admin.hobby%></th>
                                </tr>
                                <tr>
                                    <th>City</th>
                                    <th><%= locals.admin.city%></th>
                                </tr>
                                <tr>
                                    <th>Message</th>
                                    <th><%= locals.admin.message%></th>
                                </tr>
                               
                                <tr>
                                    <th>Actions</th>
                                    <th><a href="/admin/editRecord/?id=<%=locals.admin.id%>">Edit</a></th>
                                </tr>
                            </tbody>
                    </div>
                </div>
            </div>
            <%- include('footer')%>
