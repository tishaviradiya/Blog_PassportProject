
<%- include('header')%>
<!-- MAIN CONTENT-->
<div class="main-content">
    <div class="section__content section__content--p30">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-header">
                            <% if(locals.flash.success.length>0 && locals.flash.success){%>
                                <div class="alert alert-success">
                                    <%= locals.flash.success%>
                                </div>
                                <% }%>
                            <strong>Add Others Data</strong>
                        </div>
                        <div class="card-body card-block">
                            <form action="/admin/Others/insertOthersFieldData" method="post"  class="form-horizontal">
                               
                                <div class="row form-group">
                                    <div class="col col-md-3">
                                        <label for="text-input" class=" form-control-label">Name</label>
                                    </div>
                                    <div class="col-12 col-md-9">
                                        <input type="text" id="text-input" name="name" placeholder="Enter  Name" class="form-control">
                                       
                                    </div>
                                </div>
                                
                                <div class="row form-group">
                                    <div class="col col-md-3">
                                        <label for="textarea-input" class=" form-control-label">Description</label>
                                    </div>
                                    <div class="col-12 col-md-9">
                                        <textarea name="description" id="textarea-input" rows="4" placeholder="Content..." class="form-control"></textarea>
                                    </div>
                                </div>
                                <% let country=['India','Canada','Austrilia','New Zeland']%>
                                <div class="row form-group">
                                    <div class="col col-md-3">
                                        <label for="selectSm" class=" form-control-label">Select country</label>
                                    </div>
                                    <div class="col-12 col-md-9">
                                        <select name="country" id="SelectLm" class="form-control-sm form-control">
                                            <option value="">select-country</option>
                                            <% country.map((v,i)=>{%>
                                                <option value="<%= v%>"><%= v%></option>
                                           <% })%>
                                        </select>
                                    </div>
                                </div>
                                
                                <% let city=['Gujarat','Toranto','Auckland','BrrezBel']%>
                                <div class="row form-group">
                                    <div class="col col-md-3">
                                        <label for="selectSm" class=" form-control-label">Select City</label>
                                    </div>
                                    <div class="col-12 col-md-9">
                                        <select name="city" id="SelectLm" class="form-control-sm form-control">
                                            <option value="">select-city</option>
                                            <% city.map((v,i)=>{%>
                                                <option value="<%= v%>"><%= v%></option>
                                           <% })%>
                                        </select>
                                    </div>
                                </div>
                            
                        </div>
                        <div class="card-footer">
                            <button type="submit" class="btn btn-primary btn-sm">
                                <i class="fa fa-dot-circle-o"></i> Submit
                            </button>
                            <button type="reset" class="btn btn-danger btn-sm">
                                <i class="fa fa-ban"></i> Reset
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </form>
            <%- include('footer')%>
